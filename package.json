{
  "name": "shoebay",
  "version": "1.0.0",
  "main": "src/app.js",
  "repository": "https://github.com/jaseat/shoebay.git",
  "author": "jaseat <jpseatris@ucdavis.edu>",
  "license": "MIT",
  "scripts": {
    "dev-prepare": "NODE_ENV=development&sequelize db:drop&sequelize db:create&sequelize db:migrate&sequelize db:seed:all",
    "dev-prepare-win": "set NODE_ENV=development&sequelize db:drop&sequelize db:create&sequelize db:migrate&sequelize db:seed:all",
    "install": "cd client && yarn",
    "server": "nodemon src/app.js",
    "production-test": "set NODE_ENV=production&&nodemon src/app.js",
    "client": "cd client && yarn start",
    "dev": "NODE_ENV=development&&concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "devwin": "set NODE_ENV=development&&concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
    "build": "cd client && yarn build",
    "test": "mocha",
    "start": "node src/app.js",
    "heroku-postbuild": "cd client && yarn && yarn --only=dev --no-shrinkwrap && yarn build"
  },
  "dependencies": {
    "bcrypt-nodejs": "^0.0.3",
    "cookie-parser": "^1.4.3",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "express-graphql": "^0.6.12",
    "express-session": "^1.15.6",
    "graphql": "^0.13.2",
    "morgan": "^1.9.0",
    "mysql2": "^1.5.3",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "sequelize": "^4.38.0",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "concurrently": "^3.6.0",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5"
  }
}
